{
  "name": "lizhichao/one-sm",
  "type": "library",
  "description": "国密sm3",
  "keywords": [
    "php",
    "sm3"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "tanszhe",
      "email": "1018595261@qq.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "OneSm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OneSm\\Test\\": "tests/"
    }
  },
  "scripts": {
    "fixer": "php-cs-fixer fix ./",
    "test": [
      "@clearCache",
      "./vendor/bin/phpunit --colors=always"
    ],
    "clearCache": "rm -rf cache/*"
  }
}
